{
  "kind": "collectionType",
  "collectionName": "shipping_addresses",
  "info": {
    "name": "shippingAddress"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "order": {
      "model": "order",
      "via": "shipping_address"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "shippingPrice": {
      "type": "decimal"
    }
  }
}
